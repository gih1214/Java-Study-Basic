package site.metacoding.study.ex00;

// 요리사
class 임꺽정 {
	public void 요리() {
		System.out.println("요리를 시작합니다. 카레가 완성되었습니다.");
	}
}

// 사장
class 장보고 {
	임꺽정 p1;
	
	public 장보고(임꺽정 p1) {
		this.p1 = p1;
	}
}

public class MockTest {

	public static void main(String[] args) {
		임꺽정 p1 = new 임꺽정();
		new 장보고(p1);
	}

}
